{
  "group": "users",
  "name": "users.setPresence",
  "deprecated": false,
  "desc": "Manually sets user presence.",
  "args": {
    "presence": {
      "required": true,
      "example": "away",
      "desc": "Either `auto` or `away`.",
      "type": "enum"
    }
  },
  "response": {
    "examples": [
      "{\n    \"ok\": true\n}",
      "{\n    \"ok\": false,\n    \"error\": \"invalid_auth\"\n}"
    ]
  },
  "errors": {
    "invalid_presence": "Value passed for `presence` was invalid."
  }
}